set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "pkill fdns\r"
sleep 2

send -- "fdns --nofilter --server=anycast\r"
expect {
	timeout {puts "TESTING ERROR 1\n";exit}
	"anycast"
}
expect {
	timeout {puts "TESTING ERROR 2\n";exit}
	"Error" {puts "TESTING ERROR 3\n";exit}
	"SSL connection opened"
}

after 100
puts "\nall done\n"
